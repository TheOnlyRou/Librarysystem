void DeleteMember();
int id,I;
int found=0:
printf("Please enter the ID of the member you want to delete");

do {
    scanf("%d",&id);
    CheckID(id);
    I=0;
if (MembersArray[I].ID==id)
{
 found=1
}
I++;
}while(found=0 && I<100);
if (found==0)
printf("The data you entered is not acceptable");

for (I;I<99;I++){
    MembersArray[I]=MembersArray[I+1];
}

